/* pages/fiveg/index.wxss */
.page
{
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  padding-bottom: env(safe-area-inset-bottom);
}
.desc
{
  min-height: 500rpx;
}
.bom
{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top: 50rpx;
}
.yybtn
{
  background: linear-gradient(180deg, #0ED9AC 0%, #1AC89E 100%);
  border-radius: 40rpx;
  width: 580rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #ffffff;
  font-size: 28rpx;
}
.cardbtn
{
  width: 580rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #1AC99F;
  font-size: 28rpx;
  border-radius: 40rpx;
  border: 1px solid #1AC99F;
  margin-top: 30rpx;
  font-weight: 500;
}